class Solution(object):
    def singleNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        non_duplicates = []
        for i in nums:
            if i not in non_duplicates:
                non_duplicates.append(i)
            else:
                non_duplicates.remove(i)
        return non_duplicates[0]
